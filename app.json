{
  "lib": [
    "node_modules/openlayers/src/**/*.js",
    "node_modules/openlayers/build/ol.ext/**/*.js",
    "node_modules/ngeo/src/**/*.js",
    "node_modules/ngeo/exports/**/*.js",
    "src/**/*.js"
  ],
  "compile": {
    "closure_entry_point": "app",
    "externs": [
      "node_modules/openlayers/externs/bingmaps.js",
      "node_modules/openlayers/externs/closure-compiler.js",
      "node_modules/openlayers/externs/geojson.js",
      "node_modules/openlayers/externs/oli.js",
      "node_modules/openlayers/externs/olx.js",
      "node_modules/openlayers/externs/proj4js.js",
      "node_modules/openlayers/externs/tilejson.js",
      "node_modules/openlayers/externs/topojson.js",
      "node_modules/openlayers/externs/esrijson.js",
      "node_modules/ngeo/externs/closure-compiler.js",
      ".build/externs/angular-1.4.js",
      ".build/externs/angular-1.4-q_templated.js",
      ".build/externs/angular-1.4-http-promise_templated.js",
      ".build/externs/jquery-1.9.js",
      "node_modules/ngeo/externs/ngeox.js",
      "node_modules/ngeo/externs/d3.js",
      "node_modules/ngeo/externs/typeahead.js",
      "node_modules/ngeo/externs/mapfish-print-v3.js",
      "node_modules/ngeo/externs/twbootstrap.js"
    ],
    "define": [
      "goog.array.ASSUME_NATIVE_FUNCTIONS=true",
      "goog.dom.ASSUME_STANDARDS_MODE=true",
      "goog.json.USE_NATIVE_JSON=true",
      "goog.DEBUG=false",
      "ol.ENABLE_DOM=false",
      "ol.ENABLE_WEBGL=false"
    ],
    "jscomp_error": [
      "accessControls",
      "ambiguousFunctionDecl",
      "checkEventfulObjectDisposal",
      "checkRegExp",
      "checkTypes",
      "checkVars",
      "const",
      "constantProperty",
      "deprecated",
      "duplicateMessage",
      "es3",
      "es5Strict",
      "externsValidation",
      "fileoverviewTags",
      "globalThis",
      "internetExplorerChecks",
      "invalidCasts",
      "misplacedTypeAnnotation",
      "missingGetCssName",
      "missingProperties",
      "missingProvide",
      "missingReturn",
      "newCheckTypes",
      "nonStandardJsDocs",
      "suspiciousCode",
      "strictModuleDepCheck",
      "typeInvalidation",
      "undefinedNames",
      "undefinedVars",
      "uselessCode",
      "visibility"
    ],
    "jscomp_off": [
      "unknownDefines",
      "missingRequire"
    ],
    "extra_annotation_name": [
      "api", "observable", "ngdoc", "ngname", "htmlAttribute"
    ],
    "angular_pass": true,
    "compilation_level": "ADVANCED",
    "generate_exports": true,
    "warning_level": "VERBOSE",
    "output_wrapper": "(function(){%output%})();",
    "export_local_property_definitions": true,
    "use_types_for_optimization": true,
    "manage_closure_dependencies": true
  }
}
